{
  "name": "newtelco-datocms",
  "description": "NewTelco GmbH",
  "version": "0.8.0",
  "author": "Nico Domino <ndomino@newtelco.de>",
  "keywords": [
    "gatsby",
    "datocms",
    "newtelco"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "clean": "rm -rf ./node_modules && rm -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newtelco/newtelco-dato.git"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.19.14",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "autoprefixer": "^10.2.4",
    "framer-motion": "^1.11.1",
    "gatsby": "^2.23.21",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-advanced-sitemap": "^1.5.5",
    "gatsby-plugin-anchor-links": "^1.1.1",
    "gatsby-plugin-canonical-urls": "^2.3.9",
    "gatsby-plugin-emotion": "^4.3.9",
    "gatsby-plugin-google-analytics": "^2.3.10",
    "gatsby-plugin-layout": "^1.3.9",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-react-i18next": "0.0.23",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.18",
    "gatsby-plugin-sitemap": "^2.4.10",
    "gatsby-source-datocms": "^2.3.0",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-sharp": "^2.5.10",
    "gsap": "^3.5.1",
    "i18next": "^19.5.4",
    "i18next-http-backend": "^1.0.16",
    "pattern.css": "^1.0.0",
    "postcss": "^8.2.6",
    "postcss-import": "^12.0.1",
    "react": "^16.13.0",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.0",
    "react-flippy": "^0.1.5",
    "react-globe": "^5.0.2",
    "react-gsap": "^2.2.1",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.7.0",
    "react-intersection-observer": "^8.26.2",
    "react-loadable": "^5.5.0",
    "react-marquee-slider-emotion": "^1.0.17",
    "react-outside-click-handler": "^1.3.0",
    "react-parallax-tilt": "^1.4.62",
    "react-typed": "^1.2.0",
    "react-use": "^17.1.1",
    "tailwindcss": "^2.0.3",
    "three": "^0.119.1",
    "tippy.js": "^6.2.6",
    "transition-style": "^0.1.1",
    "twin.macro": "^2.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby": "^0.12.2",
    "dotenv": "^8.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "husky": "^4.2.3",
    "postcss-easings": "^2.0.0",
    "postcss-import-url": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "slug": "^3.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "strict": 0,
      "no-console": "warn",
      "quotes": [
        "warn",
        "single"
      ],
      "prettier/prettier": "warn",
      "react/prop-types": "disable",
      "no-unused-vars": "warn"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier",
      "jsx-a11y"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "parser": "babel",
    "semi": false,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js)'"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
