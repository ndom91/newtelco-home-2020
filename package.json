    "newtelco"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "clean": "rm -rf ./node_modules && rm -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newtelco/newtelco-dato.git"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.19.26",
    "autoprefixer": "^10.2.4",
    "babel-plugin-styled-components": "^1.12.0",
    "framer-motion": "^3.9.1",
    "gatsby": "^3.0.0",
    "gatsby-image": "^3.0.0",
    "gatsby-plugin-advanced-sitemap": "^1.6.0",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-canonical-urls": "^3.0.0",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-layout": "^2.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-react-i18next": "^1.0.4",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-source-datocms": "^2.3.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "gsap": "^3.6.0",
    "i18next": "^19.9.1",
    "pattern.css": "^1.0.0",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "react": "^17.0.1",
    "react-countup": "^4.3.3",
    "react-datocms": "^1.5.5",
    "react-dom": "^17.0.1",
    "react-flippy": "^0.1.5",
    "react-globe": "^5.0.2",
    "react-gsap": "^2.2.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.8",
    "react-intersection-observer": "^8.31.0",
    "react-loadable": "^5.5.0",
    "react-marquee-slider": "^1.1.2",
    "react-outside-click-handler": "^1.3.0",
    "react-parallax-tilt": "^1.4.90",
    "react-typed": "^1.2.0",
    "react-use": "^17.1.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.3",
    "three": "^0.118.3",
    "tippy.js": "^6.3.1",
    "transition-style": "^0.1.1",
    "twin.macro": "^2.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.20",
    "husky": "^5.1.3",
    "postcss-easings": "^3.0.1",
    "postcss-import-url": "^7.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.9",
    "pretty-quick": "^3.1.0",
    "slug": "^4.0.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "strict": 0,
      "no-console": "warn",
      "quotes": [
        "warn",
        "single"
      ],
      "prettier/prettier": "warn",
      "react/prop-types": "disable",
      "no-unused-vars": "warn"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier",
      "jsx-a11y"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "parser": "babel",
    "semi": false,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js)'"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
